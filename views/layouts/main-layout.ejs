    <!-- Meta tag umum untuk thumbnail -->
    <meta property="og:image" content="https://medialinadm.vercel.app/images/logo3.png" />


    <!-- Open Graph (OG) Meta Tags -->
    <meta property="og:title" content="Surat Undangan Maulid Al-Ukhuwwah Daarul Mushthofa">
